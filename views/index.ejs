    <div id="contactForm">
      <h1>Keep in touch!</h1>
      <small>We'll get back to you as quickly as possible</small>
      <form method="POST" autocomplete="off" name="Contact">
        <input placeholder="Name" name="Name" type="text" required />
        <input placeholder="Email" name="Email" type="email" required />
        <input placeholder="Subject" name="subject" type="text" required />
        <textarea placeholder="Comment" name="comment"></textarea>
        <input class="formBtn" type="submit" />
      </form>
    </div>
    <script>
      const scriptURL =
        'https://script.google.com/macros/s/AKfycbwWvG5mGFe8VyCv0cxKVhxzuxhQGQVauCg_s_JxMap65_j5l552E8ZCt7DYOrzLrwZ74A/exec'
      const form = document.forms['Contact']

      form.addEventListener('submit', e => {
        e.preventDefault()
        fetch(scriptURL, {
            method: 'POST',
            body: new FormData(form)
          })
          .then(response => alert("Thanks for Contacting us..! We Will Contact You Soon..."))
          .catch(error => console.error('Error!', error.message))
      })
    </script>

