  <div id="header">
    <h1 class="main-title">Resources</h1>
    <ul role="tablist" class="tablist">
      <li role="presentation" class="tablist__item">
        <a href="#content" role="tab" aria-selected="false" class="tablist__link">
          <img src="resource_page/images/m.png" alt="content image" class="tablist__image" />
          <figcaption class="tablist__figure-caption">Materials</figcaption>
        </a>
      </li>
      <li role="presentation" class="tablist__item">
        <a href="#blogs" role="tab" aria-selected="false" class="tablist__link">
