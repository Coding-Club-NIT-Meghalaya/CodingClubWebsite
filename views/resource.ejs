        <section class="sec" id="blogs" role="tabpanel" aria-hidden="false">
          <article class="card overflow-auto" style="display: flex">
            <div class="card__body cardblog">
              <div class="card__header">
                <h2 class="card__title">Blogs</h2>
              </div>
              <div class="cardbox">

                <% let temp = blog.length %>
                <% if(blog.length> 5){ %>
                <% temp = 5; %>
                <% } %>

                <% for(let i=0; i<temp; i++) {%>
                <a href="/resources/blog/<%= blog[i].id %> ">
                  <div class="d-flex flex-column">
                    <div className="user_name align-self-end">
                      <div class="card1">
                        <img class="card-img-top imgStyle" src="./resource_page/images/dummy.jpg"
                          alt="Card image cap" />
                        <div class="card-body">
                          <h3 class="card-title"><%= blog[i].title %> </h3>
                          <p class="card-text">
                            <%= blog[i].body %>
                          </p>
                          <p class="card-text">
                            <small class="text-muted"><%= blog[i].created %> </small>
                          </p>
                        </div>
                      </div>
                    </div>
                  </div>
                </a>
                <% } %>


              </div>
            </div>
          </article>
        </section>

        <section class="sec" id="lectures" role="tabpanel" aria-hidden="false">
          <article class="card">
            <div class="card__body">
              <div class="card__header">
                <h2 class="card__title">Videos</h2>
              </div>
              <div class="scrollmenu">
                <iframe class="iframe_video" src="https://www.youtube.com/embed/tgbNymZ7vqY?controls=0">
